<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.module.category" id="module_category_company_data">
        <field name="name">Company Data</field>
        <field name="description">Category for Company Data</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_company_data_viewer" model="res.groups">
        <field name="name">Viewer</field>
        <field name="category_id" ref="module_category_company_data"/>
    </record>

    <record id="group_company_data_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_category_company_data"/>
        <field name="implied_ids" eval="[(4, ref('group_company_data_viewer'))]"/>
    </record>

    <record id="group_company_data_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_company_data"/>
        <field name="implied_ids" eval="[(4, ref('group_company_data_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>
