<record id="rule_logistique_entry_document_viewer_readonly" model="ir.rule">
    <field name="name">Achat Viewer - Read Only Documents</field>
    <field name="model_id" ref="model_logistique_entry_document"/>
    <field name="groups" eval="[(4, ref('achat.group_purchase_viewer'))]"/>
    <field name="domain_force">[(1,'=',1)]</field>
    <field name="perm_read" eval="1"/>
    <field name="perm_write" eval="0"/>
    <field name="perm_create" eval="0"/>
    <field name="perm_unlink" eval="0"/>
</record>
