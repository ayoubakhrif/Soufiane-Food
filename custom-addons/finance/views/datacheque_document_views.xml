<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_datacheque_document_tree_inline" model="ir.ui.view">
        <field name="name">datacheque.document.tree.inline</field>
        <field name="model">datacheque.document</field>
        <field name="arch" type="xml">
            <tree string="Documents" editable="bottom" create="0" delete="1">
                <field name="doc_type" readonly="1"/>
                <field name="file_name" readonly="1"/>
                <field name="drive_url_html" widget="html" readonly="1"/>
                <field name="create_uid"/>
                <field name="create_date"/>
            </tree>
        </field>
    </record>
</odoo>
