<odoo>
    <!-- Override Login Page Logo -->
    <template id="web_login_gestia" inherit_id="web.login" priority="99">
        <xpath expr="//div[hasclass('oe_logo')]//img" position="attributes">
            <attribute name="src">/gestia_branding/static/src/img/gestia_logo.png</attribute>
            <attribute name="alt">Gestia</attribute>
        </xpath>
    </template>
</odoo>
