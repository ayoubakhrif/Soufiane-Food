<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_logistique_category" model="ir.module.category">
        <field name="name">Logistique</field>
        <field name="description">Gestion des acc√®s pour le module Logistique</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_logistique_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="module_logistique_category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_logistique_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_logistique_category"/>
        <field name="implied_ids" eval="[(4, ref('group_logistique_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>
