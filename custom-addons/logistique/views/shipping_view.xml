<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_logistique_shipping_tree" model="ir.ui.view">
        <field name="name">logistique.shipping.tree</field>
        <field name="model">logistique.shipping</field>
        <field name="arch" type="xml">
            <tree string="Shipping Companies">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_logistique_shipping_form" model="ir.ui.view">
        <field name="name">logistique.shipping.form</field>
        <field name="model">logistique.shipping</field>
        <field name="arch" type="xml">
            <form string="Shipping Company">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_logistique_shipping" model="ir.actions.act_window">
        <field name="name">Shipping Companies</field>
        <field name="res_model">logistique.shipping</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
