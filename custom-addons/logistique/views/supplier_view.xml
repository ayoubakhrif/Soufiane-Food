<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_logistique_supplier_tree" model="ir.ui.view">
        <field name="name">logistique.supplier.tree</field>
        <field name="model">logistique.supplier</field>
        <field name="arch" type="xml">
            <tree string="Suppliers">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_logistique_supplier_form" model="ir.ui.view">
        <field name="name">logistique.supplier.form</field>
        <field name="model">logistique.supplier</field>
        <field name="arch" type="xml">
            <form string="Supplier">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_logistique_supplier" model="ir.actions.act_window">
        <field name="name">Suppliers</field>
        <field name="res_model">logistique.supplier</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
